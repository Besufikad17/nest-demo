{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^generated/(.*)$": "<rootDir>/generated/$1"
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/dist",
    "<rootDir>/dist/generated"
  ]
}


